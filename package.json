{
  "name": "perfectreactapp",
  "version": "0.1.0",
  "description": "React App boilerplate with React Router 4, React 16, Webpack 4, Redux, SSR",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf dist && webpack --config webpack.config.dev.js && webpack --config webpack.config.server.dev.js",
    "build:prod": "rm -rf dist && webpack --config webpack.config.server.prod.js && webpack --config webpack.config.prod.js",
    "start": "node ./dist/server",
    "format": "prettier"
  },
  "keywords": [
    "react",
    "react-router",
    "webpack",
    "ssr",
    "redux"
  ],
  "author": "Thierry LAU",
  "license": "ISC",
  "dependencies": {
    "express": "^4.16.4",
    "history": "^4.7.2",
    "node-sass": "^4.11.0",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "sass-loader": "^7.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "prettier": "^1.16.4",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.6",
    "webpack-bundle-analyzer": "^3.1.0",
    "webpack-cli": "^3.2.3",
    "webpack-dev-middleware": "^3.6.1",
    "webpack-hot-middleware": "^2.24.3",
    "webpack-merge": "^4.2.1",
    "webpack-node-externals": "^1.7.2"
  }
}
